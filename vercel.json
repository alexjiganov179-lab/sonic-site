    { "source": "/placeholder.svg", "destination": "/assets/placeholder.svg" },
    {
      "source": "/api/projects/:project/badge-data",
      "destination": "/api/projects/:project/badge-data.json"
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/" }
  ]
